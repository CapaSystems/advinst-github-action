{
  "name": "setup-advinst",
  "version": "1.0.0",
  "private": true,
  "description": "setup advinst action",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prettier": "prettier --check **/*.ts",
    "prettier-fix": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "dist": "npm run clean && npm run build && npm run prettier && npm run lint && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/advinst-github-action.git"
  },
  "keywords": [
    "actions",
    "advist",
    "advanced installer",
    "setup",
    "download",
    "install",
    "build",
    "aip"
  ],
  "author": "Caphyon",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^1.0.8",
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/github": "^5.0.0",
    "@actions/glob": "^0.2.0",
    "@actions/http-client": "^1.0.11",
    "@actions/io": "^1.1.1",
    "@actions/tool-cache": "^1.7.1",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^12.20.37",
    "@types/semver": "^6.2.3",
    "@typescript-eslint/parser": "^5.7.0",
    "@vercel/ncc": "^0.33.0",
    "eslint": "^8.4.1",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
