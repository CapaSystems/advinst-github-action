{
  "name": "setup-advinst",
  "version": "1.0.0",
  "private": true,
  "description": "setup advinst action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist && rimraf lib",
    "rebuild": "npm run clean && npm run build",
    "prettier": "prettier --check **/*.ts",
    "prettier-fix": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint-fix": "eslint --fix src/**/*.ts",
    "pack": "ncc build ./lib/pre.js -o ./dist/pre --source-map && ncc build ./lib/main.js -o ./dist/main --source-map && ncc build ./lib/post.js -o ./dist/post --source-map",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "dist": "npm run rebuild && npm run prettier && npm run lint && npm run pack && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/advinst-github-action.git"
  },
  "keywords": [
    "actions",
    "advist",
    "advanced installer",
    "setup",
    "download",
    "install",
    "build",
    "aip"
  ],
  "author": "Caphyon",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/exec": "^1.1.0",
    "@actions/io": "^1.1.1",
    "@actions/tool-cache": "^1.7.1",
    "config-ini-parser": "^1.5.9"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.9",
    "@typescript-eslint/parser": "^5.7.0",
    "@vercel/ncc": "^0.33.0",
    "eslint": "^8.4.1",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-jest": "^25.7.0",
    "jest": "^27.4.5",
    "jest-circus": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
